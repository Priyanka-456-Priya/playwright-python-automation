# SurePrep API Test Suite - Environment Configuration
# Copy this file to .env and update with your actual credentials

# =============================================================================
# ENVIRONMENT SELECTION
# =============================================================================
ENVIRONMENT=production

# =============================================================================
# API BASE URL
# =============================================================================
# Production
SUREPREP_BASE_URL=https://api.sureprep.com

# DEVTR (Development/Test) - Uncomment to use
# SUREPREP_BASE_URL=https://devtr-api-iscrum.sureprep.com

# =============================================================================
# V5 API CREDENTIALS (Update with your production credentials)
# =============================================================================
SUREPREP_V5_USERNAME=your_v5_username
SUREPREP_V5_PASSWORD=your_v5_password
SUREPREP_V5_API_KEY=your_v5_api_key

# =============================================================================
# V7 API CREDENTIALS (Update with your production credentials)
# =============================================================================
SUREPREP_V7_CLIENT_ID=your_v7_client_id
SUREPREP_V7_CLIENT_SECRET=your_v7_client_secret

# =============================================================================
# TEST CONFIGURATION
# =============================================================================
TEST_TIMEOUT=30
DEBUG=false
